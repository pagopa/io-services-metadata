  "definitions": {
    "CitizenDTO": {
      "title": "CitizenDTO",
      "required": [
        "timestampTC"
      ],
      "type": "object",
      "properties": {
        "timestampTC": {
          "format": "date-time",
          "description": "timestamp dell'accettazione di T&C. FORMATO  ISO8601 yyyy-MM-ddTHH:mm:ss.SSSXXXXX",
          "type": "string"
        }
      }
    },
    "CitizenPatchDTO": {
      "title": "CitizenPatchDTO",
