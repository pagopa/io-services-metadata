{
  "name": "italia-services-metadata",
  "version": "1.0.15",
  "description": "Services metadata",
  "main": "src/index.ts",
  "repository": "git@github.com:pagopa/io-services-metadata.git",
  "author": "Federico Feroldi <federico@teamdigitale.governo.it>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx",
    "prettify": "prettier --write \"src/**/*.(ts|tsx)\"",
    "prettier:check": "prettier --check \"src/**/*.(ts|tsx)\"",
    "update_indicepa": "ts-node src/update_indicepa.ts",
    "update_municipalities": "ts-node src/update_municipalities.ts",
    "update_services": "ts-node src/update_services.ts",
    "check_contextual_help_data": "ts-node src/checks/checkContextualHelpData.ts",
    "check_idps_list_data": "ts-node src/checks/checkIdpsData.ts",
    "check_bonus_available_data": "ts-node src/checks/checkBonusAvailable.ts",
    "check_backend_status": "ts-node src/checks/checkBackendStatus.ts",
    "check_abi": "ts-node src/checks/checkAbi.ts",
    "check_cobadge": "ts-node src/checks/checkCoBadge.ts",
    "check_privative": "ts-node src/checks/checkPrivative.ts",
    "check_zendesk": "ts-node src/checks/checkZendesk.ts",
    "check_versionInfo": "ts-node src/checks/checkVersionInfo.ts",
    "abi-camel-case": "ts-node src/bankNamesToCamelCase.ts",
    "generate:content-definitions": "rimraf ./generated/definitions/content && mkdir -p ./generated/definitions/content && gen-api-models --api-spec definitions.yml --out-dir ./generated/definitions/content",
    "generate:pagopa-cobadge": "rimraf ./generated/definitions/pagopa/cobadge && mkdir -p ./generated/definitions/pagopa/cobadge && gen-api-models --api-spec ./pagopa/cobadge/abi_definitions.yml --out-dir ./generated/definitions/pagopa/cobadge",
    "generate:pagopa-privative": "rimraf ./generated/definitions/pagopa/privative && mkdir -p ./generated/definitions/pagopa/privative && gen-api-models --api-spec ./pagopa/privative/definitions.yml --out-dir ./generated/definitions/pagopa/privative",
    "generate:pagopa-walletv2-definitions": "rimraf generated/definitions/pagopa/walletv2 && mkdir -p generated/definitions/pagopa/walletv2 && gen-api-models --api-spec ./bonus/specs/bpd/pm/walletv2.json --out-dir ./generated/definitions/pagopa/walletv2 --no-strict --request-types --response-decoders",
    "generate:pagopa-bpd-citizen-v2": "rimraf ./generated/definitions/bpd/citizenv2 && mkdir -p ./generated/definitions/bpd/citizenv2 && gen-api-models --api-spec ./bonus/specs/bpd/citizen_v2.json --out-dir ./generated/definitions/bpd/citizenv2 --no-strict --request-types --response-decoders",
    "generate:pagopa-bpd-winning-transactions-v2": "rimraf ./generated/definitions/bpd/winningTransactionsv2 && mkdir -p generated/definitions/bpd/winningTransactionsv2 && gen-api-models --api-spec ./bonus/specs/bpd/winning_transactions_v2.json --out-dir ./generated/definitions/bpd/winningTransactionsv2 --no-strict --request-types --response-decoders",
    "generate:pagopa": "npm-run-all generate:pagopa-cobadge generate:pagopa-privative generate:pagopa-walletv2-definitions generate:pagopa-bpd-citizen-v2 generate:pagopa-bpd-winning-transactions-v2",
    "generate:all": "npm-run-all generate:content-definitions generate:pagopa",
    "run_checks": "npm-run-all check_idps_list_data check_contextual_help_data check_bonus_available_data check_backend_status check_abi check_cobadge check_privative check_zendesk check_versionInfo"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/event-stream": "^3.3.34",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "23.3.12",
    "@types/js-yaml": "^3.11.2",
    "@types/node": "^10.5.3",
    "@types/request": "^2.47.1",
    "@types/json-dup-key-validator": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-jest": "^24.8.0",
    "chalk": "^3.0.0",
    "change-case": "^4.1.2",
    "csv-parse": "^4.4.6",
    "csv-stream": "^0.2.0",
    "event-stream": "^3.3.4",
    "fs-extra": "^7.0.0",
    "italia-ts-commons": "^5.1.5",
    "italia-tslint-rules": "^1.1.0",
    "italia-utils": "^4.1.1",
    "jest": "24.8.0",
    "js-yaml": "^3.12.0",
    "json-dup-key-validator": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "request": "^2.87.0",
    "ts-jest": "24.0.1",
    "ts-node": "8.6.2",
    "eslint": "^8.6.0",
    "prettier": "2.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-functional": "^4.1.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-sonarjs": "^0.11.0",
    "typescript": "^3.7.5"
  }
}
