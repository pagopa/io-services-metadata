{
  "swagger": "2.0",
  "info": {
    "description": "RESTful API provided by the 'Payment Manager' to retrieve Cobadge Instruments",
    "title": "Cobadge Controller",
    "version": "1.6.4"
  },
  "basePath": "pp-restapi-CD/v1",
  "paths": {
    "/cobadge/add-wallets": {
      "post": {
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/CobadgeResponse"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Wallet with credit card created",
            "schema": {
              "$ref": "#/definitions/WalletV2"
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "description": "Save instruments information as credit card",
        "x-codegen-request-body-name": "body"
      }
    },
    "/cobadge/pans": {
      "get": {
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "allowEmptyValue": true,
            "in": "query",
            "name": "abiCode",
            "type": "string"
          }
        ],
        "responses": {
          "201": {
            "description": "Resource Cobadge created",
            "schema": {
              "$ref": "#/definitions/CobadgeResponse"
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "description": "Retrieve instruments information using ABI code"
      }
    },
    "/cobadge/search/{searchRequestId}": {
      "get": {
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "searchRequestId",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": {
            "description": "Resource Cobadge created",
            "schema": {
              "$ref": "#/definitions/CobadgeResponse"
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "description": "Retrieve instruments information using ABI code"
      }
    }
  },
  "definitions": {
    "CardInfo": {
      "properties": {
        "blurredNumber": {
          "type": "string"
        },
        "brand": {
          "type": "string"
        },
        "expireMonth": {
          "type": "string"
        },
        "expireYear": {
          "type": "string"
        },
        "hashPan": {
          "type": "string"
        },
        "htokenList": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "issuerAbiCode": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "CobadgeResponse": {
      "properties": {
        "errors": {
          "items": {
            "$ref": "#/definitions/ErrorModel"
          },
          "type": "array"
        },
        "payload": {
          "$ref": "#/definitions/Payload"
        },
        "status": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "ErrorModel": {
      "properties": {
        "code": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "params": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "Payload": {
      "properties": {
        "paymentInstruments": {
          "items": {
            "$ref": "#/definitions/PaymentInstrument"
          },
          "type": "array"
        },
        "searchRequestId": {
          "type": "string"
        },
        "searchRequestMetadata": {
          "items": {
            "$ref": "#/definitions/SearchRequestMetadata"
          },
          "type": "array"
        }
      },
      "type": "object"
    },
    "PaymentInstrument": {
      "properties": {
        "abiCode": {
          "type": "string"
        },
        "expiringDate": {
          "format": "date",
          "pattern": "^\\d{2}-\\d{2}-\\d{4}$",
          "type": "string"
        },
        "hpan": {
          "type": "string"
        },
        "panCode": {
          "type": "string"
        },
        "panPartialNumber": {
          "type": "string"
        },
        "paymentNetwork": {
          "enum": [
            "MAESTRO",
            "MASTERCARD",
            "VISA_ELECTRON",
            "VISA_CLASSIC",
            "VPAY"
          ],
          "type": "string"
        },
        "productType": {
          "enum": [
            "CREDIT",
            "PREPAID",
            "DEBIT",
            "PRIVATIVE"
          ],
          "type": "string"
        },
        "tokenMac": {
          "type": "string"
        },
        "validityStatus": {
          "enum": [
            "VALID",
            "BLOCK_REVERSIBLE"
          ],
          "type": "string"
        }
      },
      "type": "object"
    },
    "SearchRequestMetadata": {
      "properties": {
        "executionStatus": {
          "type": "string"
        },
        "retrievedInstrumentsCount": {
          "type": "integer"
        },
        "serviceProviderName": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "WalletV2": {
      "properties": {
        "info": {
          "$ref": "#/definitions/CardInfo"
        },
        "createDate": {
          "example": "31-01-2020 00:00:00",
          "format": "date",
          "pattern": "^\\d{2}-\\d{2}-\\d{4} \\d{2}:\\d{2}:\\d{2}$",
          "type": "string"
        },
        "enableableFunctions": {
          "items": {
            "enum": [
              "pagoPA",
              "BPD",
              "FA"
            ],
            "type": "string"
          },
          "type": "array"
        },
        "favourite": {
          "type": "boolean"
        },
        "idWallet": {
          "type": "number"
        },
        "onboardingChannel": {
          "type": "string"
        },
        "pagoPA": {
          "type": "boolean"
        },
        "updateDate": {
          "example": "31-01-2020 00:00:00",
          "format": "date",
          "pattern": "^\\d{2}-\\d{2}-\\d{4} \\d{2}:\\d{2}:\\d{2}$",
          "type": "string"
        },
        "walletType": {
          "enum": [
            "Card",
            "Bancomat",
            "Satispay",
            "BPay"
          ],
          "type": "string"
        }
      },
      "type": "object"
    }
  },
  "x-components": {}
}